     val = (ilist and jlist[j]);
   else if(op == "OR")
     val = (ilist or jlist[j]);
   endif
   var t = jlist[j];
   if (t.type == "string");t = "\"" + t + "\"";endif
   var s = "\"" + ilist.join(",") + "\".split(,) " + op + " " + t;
   if (val.type == "array");val = "\n" + val;endif
   if (val.type == "string");val = "\"" + val + "\"";endif
   print "" + ilist.type + " " +op+" " + jlist[j].type + " = " + val.type + "  -- " + s + " = " + val;
  end for
 endif

